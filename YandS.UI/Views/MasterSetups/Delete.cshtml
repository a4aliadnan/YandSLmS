@model YandS.UI.Models.MasterSetups
@{
    ViewBag.Title = "Y & S Associates Master Setup Delete";
}
<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <div class="container-fluid">
            <div class="row mb-2">
                <div class="col-sm-6">
                    <h1>Master Setup</h1>
                </div>
                <div class="col-sm-6">
                    <ol class="breadcrumb float-sm-right">
                        <li class="breadcrumb-item">Master</li>
                        <li class="breadcrumb-item active"><a href="#">Delete</a></li>
                    </ol>
                </div>
            </div>
        </div><!-- /.container-fluid -->
    </section>
    <section class="content">
        <div class="col-md-6 classCenter">
            <div class="card card-danger">
                <div class="card-header">
                    <h3 class="card-title">Master List Delete</h3>
                </div>

                <div class="card-body">
                    <p class="text-danger">Are you sure you want to delete this?</p>
                    <dl class="dl-horizontal">
                        <dt>
                            @Html.DisplayNameFor(model => model.Mst_Desc)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.Mst_Desc)
                        </dd>
                    </dl>
                    <dl class="dl-horizontal">
                        <dt>
                            @Html.DisplayNameFor(model => model.Remarks)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.Remarks)
                        </dd>
                    </dl>
                    @using (Html.BeginForm())
                    {
                        @Html.AntiForgeryToken()

                        <div class="form-actions no-color">
                            <input type="submit" value="Delete" class="btn btn-danger" /> |
                            @Html.ActionLink("Back to Main", "Index")
                        </div>
                    }
                </div>
            </div>
        </div>
    </section>
    <!-- /.content -->
</div>