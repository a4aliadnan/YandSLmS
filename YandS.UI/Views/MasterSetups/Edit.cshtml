@model YandS.UI.Models.MasterSetups
@{
    ViewBag.Title = "Y & S Associates Master Setup Modify";
}
<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <div class="container-fluid">
            <div class="row mb-2">
                <div class="col-sm-6">
                    <h1>Master Setup @ViewBag.ParentDescription</h1>
                </div>
                <div class="col-sm-6">
                    <ol class="breadcrumb float-sm-right">
                        <li class="breadcrumb-item">Master</li>
                        <li class="breadcrumb-item">@ViewBag.ParentDescription</li>
                        <li class="breadcrumb-item active"><a href="#">Create</a></li>
                    </ol>
                </div>
            </div>
        </div><!-- /.container-fluid -->
    </section>

    <!-- Main content -->
    <section class="content">

        <div class="card card-info">
            <div class="card-header">
                <h3 class="card-title">MODIFY MASTER TABLES</h3>
            </div>
            <!-- /.card-header -->
            <!-- form start -->
            <div class="card-body">


                @using (Html.BeginForm())
                {
                    @Html.AntiForgeryToken()
                    @Html.HiddenFor(model => model.MstId)
                    @Html.HiddenFor(model => model.MstParentId)
                    @Html.HiddenFor(model => model.CreatedBy)
                    @Html.HiddenFor(model => model.CreatedOn)

                    <div class="form-horizontal">
                        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                        <div class="form-group row">
                            @Html.LabelFor(model => model.Mst_Desc, htmlAttributes: new { @class = "col-sm-2 col-form-label" })
                            <div class="col-md-10">
                                @Html.EditorFor(model => model.Mst_Desc, new { htmlAttributes = new { @class = "form-control" } })
                                @Html.ValidationMessageFor(model => model.Mst_Desc, "", new { @class = "text-danger" })
                            </div>
                        </div>

                        <div class="form-group row">
                            @Html.LabelFor(model => model.Mst_Value, htmlAttributes: new { @class = "col-sm-2 col-form-label" })
                            <div class="col-md-10">
                                @Html.EditorFor(model => model.Mst_Value, new { htmlAttributes = new { @class = "form-control" } })
                                @Html.ValidationMessageFor(model => model.Mst_Value, "", new { @class = "text-danger" })
                            </div>
                        </div>

                        <div class="form-group row">
                            @Html.LabelFor(model => model.DisplaySeq, htmlAttributes: new { @class = "col-sm-2 col-form-label" })
                            <div class="col-md-10">
                                @Html.EditorFor(model => model.DisplaySeq, new { htmlAttributes = new { @class = "form-control" } })
                                @Html.ValidationMessageFor(model => model.DisplaySeq, "", new { @class = "text-danger" })
                            </div>
                        </div>
                        <div class="form-group row">
                            @Html.LabelFor(model => model.Remarks, htmlAttributes: new { @class = "col-sm-2 col-form-label" })
                            <div class="col-md-10">
                                @Html.EditorFor(model => model.Remarks, new { htmlAttributes = new { @class = "form-control" } })
                                @Html.ValidationMessageFor(model => model.Remarks, "", new { @class = "text-danger" })
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-md-offset-2 col-md-10">
                                <input type="submit" value="Edit" class="btn btn-info" />
                            </div>
                        </div>
                    </div>
                }
            </div>
            <!-- /.card-body -->
            <div class="card-footer">
                @Html.ActionLink("Back to List", "Index")
            </div>
        </div>
    </section>
    <!-- /.content -->
</div>

@section pagespecific {
    @Scripts.Render("~/bundles/jqueryval")
}