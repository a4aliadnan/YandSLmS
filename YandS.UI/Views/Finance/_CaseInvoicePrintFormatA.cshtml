@{
    decimal TotalFeeAmount = 0;
    decimal TotalVATAmount = 0;
    decimal TotalAmount = 0;

    string AccountContractNo = Model.AccountContractNo ?? string.Empty;
    string ClientFileNo = !string.IsNullOrEmpty(Model.ClientFileNo) ? " - " + Model.ClientFileNo : string.Empty;
    string ODBLoanType = !(Model.ClientCaseTypeName).Contains("PLEASE SELECT") ? " - " + Model.ClientCaseTypeName : string.Empty;
    string ODBBranchName = !(Model.ODBBranchName).Contains("PLEASE SELECT") ? " - " + Model.ODBBranchName : string.Empty;
    string strCLIENTS_NO = string.Format(@"{0}{1}{2}{3}", AccountContractNo, ClientFileNo, ODBLoanType, ODBBranchName).TrimStart('-');
    ViewBag.CLIENTS_NO = strCLIENTS_NO;

    string CaseAgainst = Model.CaseAgainst ?? string.Empty;
    string CaseNo = !string.IsNullOrEmpty(Model.CourtRefNo) ? " - " + Model.CourtRefNo : string.Empty;
    string CaseLevel = !string.IsNullOrEmpty(ViewBag.CourtType) ? " - " + ViewBag.CourtType : string.Empty;

    ViewBag.CASE_DETAILS = string.Format(@"{0}{1}{2}", CaseAgainst, CaseLevel, CaseNo).TrimStart('-');
}
