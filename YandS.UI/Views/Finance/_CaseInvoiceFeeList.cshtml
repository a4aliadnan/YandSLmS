@model IEnumerable<YandS.UI.Models.CaseInvoiceFee>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CaseInvoice.InvoiceNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FeeTypeId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FeeAmount)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CaseInvoice.InvoiceNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FeeTypeId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FeeAmount)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.FeeId }) |
                @Html.ActionLink("Details", "Details", new { id = item.FeeId }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.FeeId })
            </td>
        </tr>
    }
</table>
