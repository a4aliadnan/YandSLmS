@model YandS.UI.Models.ErrorVM
@{
    ViewBag.Title = "UnAuthorize - Error";
}
<div class="content-wrapper">
    <section class="content-header">
        <div class="container-fluid">
            <div class="row mb-2">
                <div class="col-sm-6">
                    <h1><i class="subheader-icon-err fal fa-minus-circle red"></i>Error 401 : Unauthorised Request</h1>
                </div>
                <div class="col-sm-6">
                    <ol class="breadcrumb float-sm-right">
                        <li class="breadcrumb-item">Error</li>
                        <li class="breadcrumb-item active"><a href="#">Un Authorize Access</a></li>
                    </ol>
                </div>
            </div>
        </div><!-- /.container-fluid -->
    </section>

    <!-- Main content -->
    <section class="content">
        <div id="DIV_ERROR" class="error-page">
            <h2 class="headline text-danger">Error 401</h2>
            <div class="error-content">
                <h3><i class="fas fa-exclamation-triangle text-danger"></i>Error 401 : Unauthorised Request</h3>
                <p>
                    You do not have permission to access the requested resource due to security restrictions.
                    In order to gain access, please speak to your system administrator...
                    Meanwhile, you may <a href="@Url.Action("ClientIndex", "Home")">return to Home</a>
                </p>
            </div>
        </div>
    </section>
    <!-- /.content -->
</div>