@{
    ViewBag.Title = "Y & S Associates Reset password confirmation";
}
<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <div class="container-fluid">
            <div class="row mb-2">
                <div class="col-sm-6">
                    <h1>Reset Password</h1>
                </div>
                <div class="col-sm-6">
                    <ol class="breadcrumb float-sm-right">
                        <li class="breadcrumb-item">Account</li>
                        <li class="breadcrumb-item active"><a href="#">Reset Password</a></li>
                    </ol>
                </div>
            </div>
        </div><!-- /.container-fluid -->
    </section>

    <!-- Main content -->
    <section class="content">
        <div class="card card-outline card-success">
            <div class="card-body">
                <div class="alert alert-success alert-dismissible">
                    <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
                    <h5><i class="icon fas fa-check"></i> Alert!</h5>
                    Your password has been reset.
                </div>
            </div>
        </div>
        <div class="card-footer">
            <button title="Main" class="btn btn-secondary" onclick="location.href='@Url.Action("Index","Home")';return false;"><i class="fas fa-home"></i> Home</button>
        </div>
    </section>
    <!-- /.content -->
</div>
