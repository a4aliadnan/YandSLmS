                    </table>
                </div>

                <div style="padding-bottom:10px;" class="row">
                    
                    <div class="col-sm-2">
                        <div class="text-center">
                            <h3 class="IndexTotalBG">TOTAL&nbsp;:&nbsp;<span id="TOBEREG_Total"></span></h3>
                        </div>
                    </div>
                    <div class="col-sm-10">
                        <div class="text-center">
                            <h3 style="background-color: #d9d9d9 !important; color: black !important;">TO BE REGISTER</h3>
                        </div>
                    </div>

                </div>
                <div style="padding-bottom:10px;" class="col-sm-12">
                    <table id="IndexTOBEREG" class="table table-bordered table-striped dataTable dt-responsive nowrap" style="width:100%">
                        <thead>
                            <tr>
                                <th>DAYS COUNTER</th>
                                <th class="all">FILE NO.</th>
