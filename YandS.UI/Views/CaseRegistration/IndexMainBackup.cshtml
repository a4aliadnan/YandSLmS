                <div style="padding-bottom:10px;" class="col-sm-12">
                    <table id="IndexNEWCASES" class="table table-bordered table-striped dataTable dt-responsive nowrap" style="width:100%">
                        <thead>
                            <tr>

                                <th>DAYS COUNTER</th>
                                <th class="all">FILE NO.</th>
                                <th>COURT</th>
                                <th>CLIENT</th>
                                <th>DEFENDANT</th>
