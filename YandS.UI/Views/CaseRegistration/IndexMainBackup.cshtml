                                <th class="all"></th>
                            </tr>
                        </thead>
                        <tbody></tbody>
                    </table>
                </div>

                <div style="padding-bottom:10px;" class="row">

                    <div class="col-sm-2">
                        <div class="text-center">
                            <h3 class="IndexTotalBG">TOTAL&nbsp;:&nbsp;<span id="APPEAL_Total"></span></h3>
                        </div>
                    </div>
                    <div class="col-sm-8">
                        <div class="text-center">
                            <h3 class="CaseRegAppeal">APPEAL</h3>
                        </div>
                    </div>


                    <div class="col-sm-2">
                        <div class="form-group">
                            @Html.DropDownList("ddlFilterAppeal", null, htmlAttributes: new { @class = "form-control select2bs4" })
