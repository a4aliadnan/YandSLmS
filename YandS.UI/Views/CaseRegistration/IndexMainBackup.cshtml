    </section>
    <!-- /.content -->
    <input type="hidden" id="txtUserRole" value="@ViewBag.UserRole" />
    <input type="hidden" id="txtDataFor" value="@ViewBag.DataFor" />
    <input type="hidden" id="txtLocationId" value="@ViewBag.LocationId" />

</div>


@section pagespecific {
    <script type="text/javascript">
        var GetIndexDataURL = '@Url.Action("AjaxIndexData", "CaseRegistration", new { DataFor = @ViewBag.DataFor })';
        var tableUrgent;
        var tableSupreme;
        var tableAppeal;
        var tableNewCases;
        var tableToBeRegister;
        var tableStopRegistration;
        var TableTotalURL = '@Url.Action("GetTableTotal", "CaseRegistration", new { area = string.Empty })';

        $(document).ready(function () {
            console.log("Index Case Registration Page");
            LoadDataTables();

            function LoadDataTables() {
                if (typeof tableUrgent != 'undefined')
                    tableUrgent.destroy();

                tableUrgent = $('#IndexURGENT').DataTable({
                    "processing": true,
                    "serverSide": true,
                    "filter": true,
                    "orderMulti": false,
                    "destroy": true,
                    "ordering": true,
                    "pageLength": 5,
                    //dom: 'Blfrtip',
                    dom: "<'row'<'col-sm-4'l><'col-sm-7 text-right'f><'col-sm-1 text-right'B>>" +
                        "<'row'<'col-sm-12'tr>>" +
                        "<'row'<'col-sm-5'i><'col-sm-7'p>>",
                    buttons: [
                        {
                            extend: 'excel',
                            title: 'Case Registration - URGENT CASES',
                            text: '<i class="fa fa-table fainfo" aria-hidden="true" > </i> Print',
                            titleAttr: 'Export Excel',
                            "oSelectorOpts": { filter: 'applied', order: 'current' },
                            exportOptions: { modifier: { page: 'all', search: 'none' } }
                        }
                    ],
                    "lengthMenu": [[5, 10, 25, 50, -1], [5, 10, 25, 50, 'ALL']],
                    "ajax": {
                        "url": GetIndexDataURL,
                        "type": "POST",
                        "datatype": "json",
                        data: function (d) {
                            d.DataTableName = "URGENT";
                            d.FileStatus = "A";
                            d.LocationId = $('#txtUserRole').val() == "VoucherApproval" ? $('input[name="rdoLocation"]:checked').val() : $('#txtLocationId').val();
                        }
                    },
                    "columns": [
                        { data: 'DaysCounter', name: 'DaysCounter', autoWidth: true },
                        { data: 'OfficeFileNo', name: 'OfficeFileNo', autoWidth: true },
                        { data: 'CourtRegistrationName', name: 'CourtRegistrationName', autoWidth: true },
                        { data: 'ClientName', name: 'ClientName', autoWidth: true },
                        { data: 'Defendant', name: 'Defendant', autoWidth: true },
                        { data: 'JudgementDate', name: 'JudgementDate', autoWidth: true },
                        { data: 'ActionLevelName', name: 'ActionLevelName', autoWidth: true },
                        { data: 'EnforcementDisputeName', name: 'EnforcementDisputeName', autoWidth: true },
                        { data: 'FileStatusName', name: 'FileStatusName', autoWidth: true },
                        { data: 'OnHoldReason', name: 'OnHoldReason', autoWidth: true },
                        { data: 'CourtMessage', name: 'CourtMessage', autoWidth: true },
                        { data: 'ElectronicNo', name: 'ElectronicNo', autoWidth: true },
                        {
                            data: null,
                            render: function (data, type, row) {
                                if (type === 'display') {
                                    return '<a class="btn btn-info btn-sm" title="Edit Case Registration" href="/CaseRegistration/Edit/' + row.CaseRegistrationId + '"><i class="fas fa-pencil-alt"></i></a> <a class="btn btn-danger btn-sm" title="Cancel Case Registration" href="/CaseRegistration/Delete/' + row.CaseRegistrationId + '"><i class="fas fa-trash"></i></a>';
                                }
                                return data;
                            },
                            className: "project-actions text-right"
                        }],
                    "order": [[1, "desc"]],
                    'aoColumnDefs': [
                        { 'bSortable': false, 'aTargets': [0, -1] },
                        { type: 'natural', targets: 1 }
                    ],
                    "createdRow": function (row, data, dataIndex) {
                        var rowColor = "";
                    },
                    initComplete: function () {
                        console.log("DataTable URGENT CASES Initiated !!");
                        CalculateTableTotal("URGENT");

                        $("#IndexURGENT_filter.dataTables_filter input")
                            .unbind() // Unbind previous default bindings
                            .bind("keyup", function (e) { // Bind our desired behavior
                                //console.log(e.keyCode);
                                // this.value.length >= 3 || If the length is 3 or more characters, or the user pressed ENTER, search
                                if (e.keyCode == 13) {
                                    // Call the API search function
                                    tableUrgent.search(this.value).draw();
                                }
                                // Ensure we clear the search if they backspace far enough
                                if (this.value == "") {
                                    tableUrgent.search("").draw();
                                }
                                return;
                            });

                    }
                });

                ReloadTable("tableSupreme");

                ReloadTable("tableAppeal");

                ReloadTable("tableNewCases");

                if (typeof tableToBeRegister != 'undefined')
                    tableToBeRegister.destroy();

                tableToBeRegister = $('#IndexTOBEREG').DataTable({
                    "processing": true,
                    "serverSide": true,
                    "filter": true,
                    "orderMulti": false,
                    "destroy": true,
                    "ordering": true,
                    "pageLength": 5,
                    //dom: 'Blfrtip',
                    dom: "<'row'<'col-sm-4'l><'col-sm-7 text-right'f><'col-sm-1 text-right'B>>" +
                        "<'row'<'col-sm-12'tr>>" +
                        "<'row'<'col-sm-5'i><'col-sm-7'p>>",
                    buttons: [
                        {
                            extend: 'excel',
                            title: 'Case Registration - TO BE REGISTER',
                            text: '<i class="fa fa-table fainfo" aria-hidden="true" > </i> Print',
                            titleAttr: 'Export Excel',
                            "oSelectorOpts": { filter: 'applied', order: 'current' },
                            exportOptions: { modifier: { page: 'all', search: 'none' } }
                        }
                    ],
                    "lengthMenu": [[5, 10, 25, 50, -1], [5, 10, 25, 50, 'ALL']],
                    "ajax": {
                        "url": GetIndexDataURL,
                        "type": "POST",
                        "datatype": "json",
                        data: function (d) {
                            d.DataTableName = "TOBEREG";
                            d.FileStatus = "A";
                            d.LocationId = $('#txtUserRole').val() == "VoucherApproval" ? $('input[name="rdoLocation"]:checked').val() : $('#txtLocationId').val();
                        }
                    },
                    "columns": [
                        { data: 'DaysCounter', name: 'DaysCounter', autoWidth: true },
                        { data: 'OfficeFileNo', name: 'OfficeFileNo', autoWidth: true },
                        { data: 'CourtRegistrationName', name: 'CourtRegistrationName', autoWidth: true },
                        { data: 'ClientName', name: 'ClientName', autoWidth: true },
                        { data: 'Defendant', name: 'Defendant', autoWidth: true },
                        { data: 'ReceptionDate', name: 'ReceptionDate', autoWidth: true }],
                    "order": [[1, "desc"]],
                    'aoColumnDefs': [
                        { 'bSortable': false, 'aTargets': [0] },
                        { type: 'natural', targets: 1 }
                    ],
                    "createdRow": function (row, data, dataIndex) {
                        var rowColor = "";
                    },
                    initComplete: function () {
                        console.log("DataTable TO BE REGISTER CASES Initiated !!");
                        CalculateTableTotal("TOBEREG");

                        $("#IndexTOBEREG_filter.dataTables_filter input")
                            .unbind() // Unbind previous default bindings
                            .bind("keyup", function (e) { // Bind our desired behavior
                                //console.log(e.keyCode);
                                // this.value.length >= 3 || If the length is 3 or more characters, or the user pressed ENTER, search
                                if (e.keyCode == 13) {
                                    // Call the API search function
                                    tableToBeRegister.search(this.value).draw();
                                }
                                // Ensure we clear the search if they backspace far enough
                                if (this.value == "") {
                                    tableToBeRegister.search("").draw();
                                }
                                return;
                            });
                    }
                });

                if (typeof tableStopRegistration != 'undefined')
                    tableStopRegistration.destroy();

                tableStopRegistration = $('#IndexSTOPREG').DataTable({
                    "processing": true,
                    "serverSide": true,
                    "filter": true,
                    "orderMulti": false,
                    "destroy": true,
                    "ordering": true,
                    "pageLength": 5,
                    //dom: 'Blfrtip',
                    dom: "<'row'<'col-sm-4'l><'col-sm-7 text-right'f><'col-sm-1 text-right'B>>" +
                        "<'row'<'col-sm-12'tr>>" +
                        "<'row'<'col-sm-5'i><'col-sm-7'p>>",
                    buttons: [
                        {
                            extend: 'excel',
                            title: 'Case Registration - STOP REGISTRATION',
                            text: '<i class="fa fa-table fainfo" aria-hidden="true" > </i> Print',
                            titleAttr: 'Export Excel',
                            "oSelectorOpts": { filter: 'applied', order: 'current' },
                            exportOptions: { modifier: { page: 'all', search: 'none' } }
                        }
                    ],
                    "lengthMenu": [[5, 10, 25, 50, -1], [5, 10, 25, 50, 'ALL']],
                    "ajax": {
                        "url": GetIndexDataURL,
                        "type": "POST",
                        "datatype": "json",
                        data: function (d) {
                            d.DataTableName = "STOPREG";
                            d.FileStatus = "A";
                            d.LocationId = $('#txtUserRole').val() == "VoucherApproval" ? $('input[name="rdoLocation"]:checked').val() : $('#txtLocationId').val();
                        }
                    },
                    "columns": [
                        { data: 'DaysCounter', name: 'DaysCounter', autoWidth: true },
                        { data: 'CourtRegistrationName', name: 'CourtRegistrationName', autoWidth: true },
                        { data: 'ElectronicNo', name: 'ElectronicNo', autoWidth: true },
                        { data: 'OfficeFileNo', name: 'OfficeFileNo', autoWidth: true },
                        { data: 'ClientName', name: 'ClientName', autoWidth: true },
                        { data: 'Defendant', name: 'Defendant', autoWidth: true },
                        { data: 'ReasonOfStopRegistration', name: 'ReasonOfStopRegistration', autoWidth: true },
                        { data: 'CourtMessage', name: 'CourtMessage', autoWidth: true },
                        {
                            data: null,
                            render: function (data, type, row) {
                                if (type === 'display') {
                                    return '<a class="btn btn-info btn-sm" title="Edit Case Registration" href="/CaseRegistration/Edit/' + row.CaseRegistrationId + '"><i class="fas fa-pencil-alt"></i></a> <a class="btn btn-danger btn-sm" title="Cancel Case Registration" href="/CaseRegistration/Delete/' + row.CaseRegistrationId + '"><i class="fas fa-trash"></i></a>';
                                }
                                return data;
                            },
                            className: "project-actions text-right"
                        }],
                    "order": [[3, "desc"]],
                    'aoColumnDefs': [
                        { 'bSortable': false, 'aTargets': [0, -1] },
                        { type: 'natural', targets: 3 }
                    ],
                    "createdRow": function (row, data, dataIndex) {
                        var rowColor = "";
                    },
                    initComplete: function () {
                        console.log("DataTable STOP REGISTRATIONI Initiated !!");
                        CalculateTableTotal("STOPREG");

                        $("#IndexSTOPREG_filter.dataTables_filter input")
                            .unbind() // Unbind previous default bindings
                            .bind("keyup", function (e) { // Bind our desired behavior
                                //console.log(e.keyCode);
                                // this.value.length >= 3 || If the length is 3 or more characters, or the user pressed ENTER, search
                                if (e.keyCode == 13) {
                                    // Call the API search function
                                    tableStopRegistration.search(this.value).draw();
                                }
                                // Ensure we clear the search if they backspace far enough
                                if (this.value == "") {
                                    tableStopRegistration.search("").draw();
                                }
                                return;
                            });
                    }
                });

            }

            function ReloadTable(table) {

                if (table == "tableSupreme") {
                    if (typeof tableSupreme != 'undefined')
                        tableSupreme.destroy();

                    tableSupreme = $('#IndexSUPREME').DataTable({
                        "processing": true,
                        "serverSide": true,
                        "filter": true,
                        "orderMulti": false,
                        "destroy": true,
                        "ordering": true,
