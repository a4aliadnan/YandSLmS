                                <th>CLIENT</th>
                                <th>DEFENDANT</th>
                                <th>RECEIVED DATE</th>
                            </tr>
                        </thead>
                        <tbody></tbody>
                    </table>
                </div>

                <div style="padding-bottom:10px;" class="row">
                    <div class="col-sm-2 AppHidden">
                        <div class="text-center">
                            <h3 class="IndexTotalBG">TOTAL&nbsp;:&nbsp;<span id="STOPREG_Total">@ViewBag.STOPREG_MCTTotal</span></h3>
                        </div>
                    </div>
                    <div class="col-sm-12">
                        <div class="text-center">
                            <h3 class="bg-black">STOP REGISTRATION</h3>
                        </div>
                    </div>

                </div>
                <div style="padding-bottom:10px;" class="col-sm-12">
                    <table id="IndexSTOPREG" class="table table-bordered table-striped dataTable dt-responsive nowrap" style="width:100%">
                        <thead>
                            <tr>
                                <th>DAYS COUNTER</th>
                                <th>COURT</th>
                                <th>ELECTRONIC NO.</th>
