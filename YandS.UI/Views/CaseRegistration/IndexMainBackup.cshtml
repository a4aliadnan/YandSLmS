@{
    ViewBag.Title = "Y & S Associates - Case Registration List";
}
<style>
    div.dt-buttons {
        width: auto !important;
    }
</style>
<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <div class="container-fluid">
            <div class="row mb-2">
                <div style="background-color: #ffff00 !important; color: black !important;padding-top:10px;" class="col-sm-3 text-center">
                    <h1>REGISTRATION IN PROGRESS</h1>
                </div>
                @if (User.IsInRole("VoucherApproval"))
                {
                    <div class="col-sm-1"></div>
                    <div style="background-color: #ffff00 !important; color: black !important;padding-top:10px;" class="col-sm-3 text-center">
                        <div class="form-group clearfix">
                            <div class="icheck-primary d-inline">
                                <input type="radio" id="radioPrimary1" value="A" name="rdoLocation" @ViewBag.VoucherApproval>
                                <label for="radioPrimary1"> ALL </label>
                            </div>
                            <div class="icheck-primary d-inline">
                                <input type="radio" id="radioPrimary2" value="M" name="rdoLocation">
                                <label for="radioPrimary2"> MUSCAT </label>
                            </div>
                            <div class="icheck-primary d-inline">
                                <input type="radio" id="radioPrimary3" value="S" name="rdoLocation">
                                <label for="radioPrimary3"> SALALAH </label>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-1"></div>
                }
                else
                {
                    <div class="col-sm-5 text-center">

                    </div>

                }
                <div class="col-sm-4">
                    <ol class="breadcrumb float-sm-right">
                        <li class="breadcrumb-item">Case Registration In Progress</li>
                        <li class="breadcrumb-item active"><a href="#">Main List</a></li>
                    </ol>

                </div>
            </div>
        </div><!-- /.container-fluid -->
    </section>

    <!-- Main content -->
    <section class="content">
        <div class="card card-primary card-outline">
            <div class="card-header">
                <h3 class="card-title"><span class="IndexTotalBG btn-lg btn-flat disabled">TO BE REGISTER</span>&nbsp;<span style="background-color: #fbe5d6 !important; color:black !important;" class="btn-lg btn-flat bg-maroon disabled" id="TTL_TBR"></span>&nbsp;<span class="IndexTotalBG btn-lg btn-flat disabled">NEW CASES</span>&nbsp;<span style="background-color: #fbe5d6 !important; color:black !important;" class="btn-lg btn-flat bg-maroon disabled" id="TTL_NEWCASES"></span> </h3>
                <div class="card-tools">
                    <button title="REGISTRATION" style="background-color: #92d050 !important;" class="btn btn-flat btn-lg bg-light" onclick="location.href='@Url.Action("Create", "CaseRegistration")';return false;">REGISTRATION</button>
                </div>
            </div>

            <div class="card-body">
                <div style="padding-bottom:10px;" class="row">
                   
                    <div class="col-sm-2">
                        <div class="text-center">
                            <h3 class="IndexTotalBG">TOTAL&nbsp;:&nbsp;<span id="URGENT_Total">@ViewBag.URGENT_Total</span></h3>

                        </div>
                    </div>
                    <div class="col-sm-10">
                        <div class="text-center">
                            <h3 style="background-color: #ff2d2d !important; color: white !important;">URGENT CASES</h3>
                        </div>
                    </div>

                </div>
                <div style="padding-bottom:10px;" class="col-sm-12">
                    <table id="IndexURGENT" class="table table-bordered table-striped dataTable dt-responsive nowrap" style="width:100%">
                        <thead>
                            <tr>

                                <th>DAYS COUNTER</th>
                                <th class="all">FILE NO.</th>
                                <th>COURT</th>
                                <th>CLIENT</th>
                                <th>DEFENDANT</th>
                                <th>JUDGEMENT DATE</th>
                                <th>ACTION LEVEL</th>
                                <th>ENFORCEMENT DISPUTE</th>
                                <th>FILE STATUS</th>
                                <th>ON HOLD REASON</th>
                                <th>COURT MESSAGE</th>
                                <th>ELECTRONIC NO.</th>
                                <th class="all"></th>
                            </tr>
                        </thead>
                        <tbody></tbody>
                    </table>
                </div>

                <div style="padding-bottom:10px;" class="row">
                   
                    <div class="col-sm-2">
                        <div class="text-center">
                            <h3 class="IndexTotalBG">TOTAL&nbsp;:&nbsp;<span id="SUPREME_Total"></span></h3>
                        </div>
                    </div>
                    <div class="col-sm-8">
                        <div class="text-center">
                            <h3 class="CaseRegSupreme">SUPREME</h3>
                        </div>
                    </div>

                    <div class="col-sm-2">
