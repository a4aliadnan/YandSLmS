                            <select id="ddlFilterSupreme" class="form-control select2 select2-danger" data-dropdown-css-class="select2-danger" style="width: 100%;">
                                <option value="A" selected="selected">ALL</option>
                                <option value="2">WRITING SUBMISSION</option>
                                <option value="3">ON HOLD</option>
                                <option value="5">SCANNED</option>
                                <option value="6">ONLINE SUBMISSION</option>
                                <option value="7">PAYMENT</option>
                                <option value="10">STOP REGISTRATION</option>
                            </select>
                        </div>*@
                        <div class="form-group">
                            @Html.DropDownList("ddlFilterSupreme", null, htmlAttributes: new { @class = "form-control select2bs4" })
                        </div>


                    </div>

                </div>
                <div style="padding-bottom:10px;" class="col-sm-12">
                    <table id="IndexSUPREME" class="table table-bordered table-striped dataTable dt-responsive nowrap" style="width:100%">
                        <thead>
                            <tr>

                                <th>DAYS COUNTER</th>
